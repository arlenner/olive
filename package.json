{
  "name": "olive-spa",
  "version": "4.0.4",
  "description": "Tiny vanilla-based SPA framework.",
  "main": "dist/index.min.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "webpack-dev-server --port 8080 --open --config webpack.config.js",
    "test": "jest",
    "lint": "eslint *.js src/*.js src/*/*.js ",
    "precommit": "yarn build && yarn lint && yarn test",
    "prepush": "yarn lint && yarn test"
  },
  "files": [
    "index.min.js",
    "index.d.ts"
  ],
  "devDependencies": {
    "typescript": "^4.3.2"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/"
  },
  "author": "Alexander Ross Lennert <arlenross02@gmail.com> (http://arlenner.github.io/portfolio)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arlenner/olive/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olive"
  },
  "homepage": "https://github.com/olive#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "SPA",
    "framework",
    "react",
    "vanilla"
  ]
}
